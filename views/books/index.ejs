<h2>Search book</h2>
<form action="/books" method="get">
    <div>
        <label>
            title
        </label>
        <input type="text" name="title" value="<%= Search.title %>">
    </div>
    <div>
        <label>
            publishDateBefore
        </label>
        <input type="date" name="publishdatebefore" value="<%= Search.publishDateBefore %>">
    </div>
    <div>
        <label>
            publishDateAfter
        </label>
        <input type="date" name="publishdateafter" value="<%= Search.publishDateAfter %>">
    </div>
    <div>
        <button type="submit">
            search
        </button>
    </div>
    <div>
        <% books.map((i) => { %>
                <a href="/books/<%= i.id %>">
                <img src="<%= i.coverImagePath %>" height="100px" width="150px">
            </a>
            <% }) %>
    </div>
</form>